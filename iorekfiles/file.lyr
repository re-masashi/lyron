class File{
	def File(
		self: File, 
		path: Str, 
		# permissions: Str
	)->None do
		self = setattr(self, "path", path);
		# self = setattr(self, "permissions", permissions);
		self
	end

	def read(self: Self)->Str do
		read_file(self.path);
	end

	def write(self:Self, data: Str)->Any do
		write_file(self.path, data);
	end
}